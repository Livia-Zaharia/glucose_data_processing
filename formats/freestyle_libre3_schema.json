{
  "database": "freestyle_libre3",
  "timestamp_formats": [
    "%d-%m-%Y %H:%M",
    "%d-%m-%Y %H:%M:%S"
  ],
  "timestamp_output_format": "%Y-%m-%dT%H:%M:%S",
  "remove_after_calibration": true,
  "field_categories": {
    "timestamp": "service",
    "event_type": "service",
    "glucose_value_mgdl": "continuous",
    "fast_acting_insulin_u": "occasional",
    "long_acting_insulin_u": "occasional",
    "carb_grams": "occasional"
  },
  "converters": {
    "libre3": {
      "timestamp_field": "Device Timestamp",
      "field_mappings": {
        "Device Timestamp": "timestamp",
        "Record Type": "record_type",
        "Historic Glucose mg/dL": "glucose_value_mgdl",
        "Scan Glucose mg/dL": "glucose_value_mgdl",
        "Rapid-Acting Insulin (units)": "rapid_acting_insulin_u",
        "Meal Insulin (units)": "meal_insulin_u",
        "Correction Insulin (units)": "correction_insulin_u",
        "Long-Acting Insulin (units)": "long_acting_insulin_u",
        "Carbohydrates (grams)": "carb_grams"
      }
    }
  }
}

